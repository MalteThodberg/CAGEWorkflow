issn = {1362-4962},
journal = {Nucleic acids research},
month = {dec},
pages = {1--21},
pmid = {30566651},
publisher = {Oxford University Press},
title = {{Comprehensive profiling of the fission yeast transcription start site activity during stress and media response.}},
