issn = {1362-4962},
journal = {Nucleic acids research},
month = {apr},
number = {7},
pages = {e47},
pmid = {25605792},
title = {{limma powers differential expression analyses for RNA-sequencing and microarray studies.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25605792 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4402510},
volume = {43},
